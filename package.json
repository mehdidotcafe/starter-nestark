{
  "name": "nestark",
  "version": "0.2.0",
  "private": false,
  "license": "GPL-3.0-or-later",
  "scripts": {
    "dev": "dotenv -- bash -c 'next dev -p $APP_PORT'",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .tsx,.ts,.js",
    "lint:fix": "eslint . --ext .tsx,.ts,.js --fix",
    "lint-staged": "lint-staged",
    "test:e2e": "dotenv -- bash -c 'cypress open --e2e'",
    "test:e2e:ci": "dotenv -e .env.test -- bash -c 'start-server-and-test dev $APP_URL \"cypress run --e2e\"'",
    "test": "jest --watchAll",
    "test:ci": "jest --coverage",
    "prepare": "husky install"
  },
  "dependencies": {
    "next": "13.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-intl": "^6.3.2",
    "styled-components": "^5.3.9"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@types/react": "^18.2.7",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "cypress": "^12.10.0",
    "dotenv-cli": "7.2.1",
    "eslint": "^8.38.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-next": "13.3.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "^13.2.1",
    "start-server-and-test": "^2.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^4.9.4"
  },
  "lint-staged": {
    "**/*.{ts,tsx,js}": [
      "npm run lint:fix"
    ]
  }
}
